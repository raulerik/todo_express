<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>To-Do Express</title>
<!-- Compiled and minified CSS -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
</head>
<body>
<div class="container">

    <div class="row">
        <div class="col s12 m6">
            <div class="card">
                <div class="card-content">
                <span class="card-title">To-Do List</span>
                <div class="row">
                    <form class="col s12">
                        <div class="row">
                            <div class="input-field col s12">
                                <input type="text" id="task" placeholder="Task">
                                <label for="task">Insert Task</label>
                            </div>
                        </div>
                        <input class="btn" type="submit" values="Add Task">
                    </form>
                </div>
                <div class="card-action">
                    <ul class="collection">
                        <li class="collection-item">Study HTML</li>
                        <li class="collection-item">Study CSS</li>
                        <li class="collection-item">Study JS</li>
                        <li class="collection-item">Study OOP</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

</div>
<!-- Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</body>
</html>